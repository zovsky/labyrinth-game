    <string name="option_212_1">287</string>
    <string name="text_212_1">На запад</string>
    <string name="option_212_2">38</string>
    <string name="text_212_2">На север</string>
    <string name="option_212_3">82</string>
    <string name="text_212_3">На восток</string>
    <string name="option_212_4">336</string>
    <string name="text_212_4">На юг</string>
